{
  "name": "sapphire",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Rudy Clark <rudy.uzb@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run client:start\" \"npm run server:start\"",
    "client:start": "cd client && npm run start",
    "server:start": "cross-env NODE_ENV=development nodemon --exec babel-node src/app.js",
    "server:build": "rimraf ./build && babel src --out-dir build",
    "server:build:min": "rimraf ./build && babel src --out-dir build --presets minify",
    "serve": "npm run server:build && node build/app.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-preset-minify": "^0.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "keygrip": "^1.1.0",
    "knex": "^0.16.5",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-passport": "^4.1.3",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.0",
    "pg": "^7.11.0"
  }
}
